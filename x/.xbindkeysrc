"/usr/bin/slock"
    Control+Alt + l

"/usr/bin/amixer set Master 5%+"
    XF86AudioRaiseVolume

"/usr/bin/amixer set Master 5%-"
    XF86AudioLowerVolume

"/usr/bin/amixer set Master toggle"
    XF86AudioMute

"/usr/bin/xbacklight -inc 10"
    XF86MonBrightnessUp

"/usr/bin/xbacklight -dec 10"
    XF86MonBrightnessDown
