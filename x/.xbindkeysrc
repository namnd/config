"/usr/bin/slock"
    Control+Alt + l

"pamixer -i 5; sigdwmblocks 2"
    XF86AudioRaiseVolume

"pamixer -d 5; sigdwmblocks 2"
    XF86AudioLowerVolume

"mute=$(pamixer --get-mute); if ($mute); then pamixer -u; else pamixer -m; fi; sigdwmblocks 2"
    XF86AudioMute

"/usr/bin/xbacklight -inc 10"
    XF86MonBrightnessUp

"/usr/bin/xbacklight -dec 10"
    XF86MonBrightnessDown
