"/usr/bin/slock"
    Control+Alt + l

"amixer set Master 5%+; pkill -RTMIN+2 goblocks"
    XF86AudioRaiseVolume

"amixer set Master 5%-; pkill -RTMIN+2 goblocks"
    XF86AudioLowerVolume

"amixer set Master toggle; pkill -RTMIN+2 goblocks"
    XF86AudioMute

"/usr/bin/xbacklight -inc 10"
    XF86MonBrightnessUp

"/usr/bin/xbacklight -dec 10"
    XF86MonBrightnessDown
